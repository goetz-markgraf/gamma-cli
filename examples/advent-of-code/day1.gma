    }


let count-increase list =
    let ret! = 0
    let current! = 10000

    list
    |> for-each [ i ->
         i > current! ? (set ret! = ret! + 1) : ()
         set current! = i ]

    ret!

print (input |> count-increase)

read-lines "gamma-cli/examples/advent-of-code/day1.input" |> count-increase
